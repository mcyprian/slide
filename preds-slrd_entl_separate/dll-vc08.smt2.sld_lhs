RootCall LHS(x_emp,y_emp,z_emp,w_emp,u_emp,t_emp,nil) 


LHS(x_emp,y_emp,z_emp,w_emp,u_emp,t_emp,newn) ::= w_emp->t_emp,u_emp & x_emp!=w_emp & w_emp!=t_emp & z_emp!=w_emp * dll(x_emp,u_emp,newn,w_emp) * dll(t_emp,y_emp,w_emp,z_emp)


dll(fr,bk,pr,nx) ::=  fr=nx & bk=pr & emp |
  \E u . fr->u,pr & fr!=nx & bk!=pr * dll(u,bk,fr,nx)
